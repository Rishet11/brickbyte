{
  "version": 2,
  "framework": "nextjs",
  "rootDirectory": "frontend",
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "installCommand": "npm install",
  "env": {
    "NEXT_PUBLIC_API_URL": "https://brickbyte-backend.onrender.com",
    "NEXT_PUBLIC_CHATBASE_API_KEY": "@chatbase_api_key",
